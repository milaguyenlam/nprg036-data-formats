#categories
<#seasonal>
    a schema:ProductGroup ;
    schema:name "Seasonal"@en ;
    schema:alternateName "Sezonni"@cs .
<#fresh>
    a schema:ProductGroup ;
    schema:name "Fresh"@en ;
    schema:alternateName "Cerstve"@cs ;
    schema:alternateName "Tuoi"@vi .
    
<#drinks>
    a schema:ProductGroup ;
    schema:name "Drinks"@en ;
    schema:alternateName "Napoje"@cs ;
    schema:alternateName "Do uong"@vi .
<#dairy>
    a schema:ProductGroup ;
    schema:name "Dairy"@en ;
    schema:alternateName "Mlecne vyrobky"@cs ; 
    schema:isPartOf <#fresh> .
<#alcohol>
    a schema:ProductGroup ;
    schema:name "Alcoholic drinks"@en ;
    schema:alternateName "Alkoholicke napoje"@cs ;
    schema:isPartOf <#drinks> .
<#fruit>
    a schema:ProductGroup ;
    schema:name "Fruit"@en ;
    schema:alternateName "ovoce"@cs ;
    schema:isPartOf <#fresh> .

