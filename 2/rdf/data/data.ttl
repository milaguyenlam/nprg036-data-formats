    schema:image <#http://schema.org/ImageObject/1> ;
    schema:category <#fruit> ;
    schema:category <#seasonal> .
<#http://schema.org/Product/2>
    a schema:Product ;
    schema:name "Milk"@en ;
    schema:gtin13 "124124124124" ;
    schema:description "Fresh milk from an organic farm"@en ;
    schema:rating 5 ;
    schema:rating 2 ;
    schema:rating 4 ;
    schema:manufacturer <#agrofert> ;
    schema:image <#http://schema.org/ImageObject/2> ;
    schema:category <#dairy> .
<#http://schema.org/Product/3>
    a schema:Product ;
    schema:name "Pilsner beer"@en ;
    schema:gtin13 "321321321321" ;
    schema:description "Authentic Czech beer"@en ;
    schema:rating 5 ;
